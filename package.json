{
  "name": "formaldoc",
  "private": true,
  "version": "1.5.0",
  "type": "module",
  "bin": {
    "formaldoc": "./cli/index.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src cli",
    "lint:fix": "eslint src cli --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"cli/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" \"cli/**/*.ts\"",
    "test": "bun test",
    "test:fast": "bun test src/",
    "test:watch": "bun test --watch",
    "cli": "bun run cli/index.ts",
    "cli:link": "bun link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shrektan/formaldoc.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/katex": "^0.16.7",
    "@types/turndown": "^5.0.6",
    "@vercel/analytics": "^1.6.1",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jsdom": "^26.1.0",
    "katex": "^0.16.27",
    "mathml2omml": "^0.5.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "^1.3.5",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0"
  }
